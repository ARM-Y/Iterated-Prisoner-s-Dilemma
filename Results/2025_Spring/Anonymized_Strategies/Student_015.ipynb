                "def Student_015(history):\n",
                "\n",
                "    if not history: # \u062f\u0648\u0631 \u0627\u0648\u0644\n",
                "        return \"Cooperate\"\n",
                "\n",
                "    last_opponent_move = history[-1][1] # \u062d\u0631\u06a9\u062a \u0622\u062e\u0631 \u0631\u0642\u06cc\u0628\n",
                "\n",
                "    if last_opponent_move == \"Cooperate\":\n",
                "        return \"Cooperate\"\n",
                "    else:\n",
                "        return \"Defect\""
            ]
        }
